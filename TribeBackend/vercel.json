{
    "version": 2,
    "builds": [
        {
        "src": "server/index.js",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "server/index.js"
        }
    ],
    "github": {
        "enabled": true,
        "autoJobCancelation": true,
        "silent": false,
        "deploymentEnabled": {
            "main": true,
            "*": false
        }
    }
}